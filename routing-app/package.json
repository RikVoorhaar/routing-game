{
	"name": "routing-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"format:staged": "sh scripts/format-staged.sh",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"init-db": "NODE_OPTIONS='--import=tsx' node scripts/init-db.ts",
		"init-db:force": "NODE_OPTIONS='--import=tsx' node scripts/init-db.ts --force",
		"setup-postgis": "NODE_OPTIONS='--import=tsx' node scripts/setup-postgis.ts",
		"analyze-addresses": "tsx src/scripts/analyze-address-distribution.ts",
		"generate-auth-secret": "node scripts/generate-auth-secret.js",
		"job-generator": "tsx src/scripts/job-generator.ts",
		"knip": "knip",
		"knip:production": "knip --production"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/geojson": "^7946.0.13",
		"@types/js-yaml": "^4.0.9",
		"@types/leaflet": "^1.9.18",
		"@types/node": "^22",
		"@types/pg": "^8.11.20",
		"autoprefixer": "^10.4.21",
		"daisyui": "^4.12.24",
		"drizzle-kit": "^0.30.6",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"knip": "^5.78.0",
		"postcss": "^8.5.4",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"pino-pretty": "^13.1.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"tsx": "^4.20.3",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@auth/core": "^0.39.0",
		"@auth/drizzle-adapter": "^1.9.1",
		"@auth/sveltekit": "^1.9.2",
		"@faker-js/faker": "^9.8.0",
		"@node-rs/argon2": "^2.0.2",
		"@turf/turf": "^7.2.0",
		"@types/cli-progress": "^3.11.6",
		"cli-progress": "^3.12.0",
		"drizzle-orm": "^0.43.1",
		"pino": "^9.5.0",
		"rotating-file-stream": "^3.2.1",
		"js-yaml": "^4.1.1",
		"leaflet": "^1.9.4",
		"nanoid": "^5.1.5",
		"pg": "^8.13.1",
		"postgres": "^3.4.5",
		"dotenv": "^16.5.0",
		"redis": "^4.7.0"
	}
}
